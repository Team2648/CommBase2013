package org.usfirst.frc2648.CommBase2013.commands;
import edu.wpi.first.wpilibj.command.CommandGroup;
import org.usfirst.frc2648.CommBase2013.Robot;
import org.usfirst.frc2648.CommBase2013.RobotMap;

/**
 *
 */
public class LEDFlicker extends CommandGroup {
    
    public  LEDFlicker() {
        addSequential(new setLED(RobotMap.lEDControlunderShooter, true));
        addSequential(new SitWait(.5));
        addSequential(new setLED(RobotMap.lEDControlunderShooter, false));
        addSequential(new setLED(RobotMap.lEDControlunderTier1_1, true));
        addSequential(new setLED(RobotMap.lEDControlunderTier1_2, true));
        addSequential(new SitWait(.5));
        addSequential(new setLED(RobotMap.lEDControlunderTier1_1, false));
        addSequential(new setLED(RobotMap.lEDControlunderTier1_2, false));
        addSequential(new setLED(RobotMap.lEDControlunderTier2_1, true));
        addSequential(new setLED(RobotMap.lEDControlunderTier2_2, true));
        addSequential(new SitWait(.5));
        addSequential(new setLED(RobotMap.lEDControlunderTier2_1, false));
        addSequential(new setLED(RobotMap.lEDControlunderTier2_2, false));
        addSequential(new SitWait(.5));
        
    }
}
